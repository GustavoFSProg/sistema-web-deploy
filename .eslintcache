a.append('price', price)\n\n    const data = {\n      title,\n      price,\n      description,\n    }\n\n    try {\n      await api.put(`/update/${id}`, data)\n\n      alert('Editado realizado com sucesso!')\n      return history.push('/')\n    } catch (error) {\n      console.log(error)\n      return alert(`Deu erro no front ${error}`)\n    }\n  }\n\n  return (\n    <div className=\"container\">\n      <Header />\n\n      <form onSubmit={handleSubmit} className=\"janela\">\n        <div className=\"profile-container\">\n          <fieldset>\n            <legend>Editar Produto</legend>\n\n            <div className=\"input-block\">\n              <p key={lista.id}> </p>\n              <label htmlFor=\"name\">Titulo</label>\n              <input\n                id=\"title\"\n                value={title}\n                placeholder={lista.title}\n                onChange={(e) => setTitle(e.target.value)}\n              />\n            </div>\n            <div className=\"input-block\">\n              <strong>Descrição</strong>\n              <input\n                id=\"description\"\n                value={description}\n                placeholder={lista.description}\n                onChange={(e) => setDescription(e.target.value)}\n              />\n            </div>\n\n            <div className=\"input-block\">\n              <strong>Preço:</strong>\n\n              <input\n                id=\"price\"\n                value={price}\n                placeholder={lista.price}\n                onChange={(e) => setPrice(e.target.value)}\n              />\n            </div>\n\n            <button type=\"submit\">EDITAR</button>\n          </fieldset>\n        </div>\n      </form>\n    </div>\n  )\n}\n","/home/gustavo/dev/git/sistema-web-deploy/src/routes.js",[],"/home/gustavo/dev/git/sistema-web-deploy/src/service/api.js",[],"/home/gustavo/dev/git/sistema-web-deploy/src/pages/Home/index.js",["48"],"/home/gustavo/dev/git/sistema-web-deploy/src/pages/Header/index.